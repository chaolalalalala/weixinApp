.c-swiper{
  width: 100%;
  height:400rpx;
}
.c-swiper image{
  width: 100%;
  height:400rpx;
  display: block;
}
.c-con{
  width: 700rpx;
  height: auto;
  overflow: hidden;
  margin: 0 auto;
  padding-bottom: 130rpx;
}
.c-welfareType{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 40rpx;
  color:#040404;
}
.c-time{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 32rpx;
  color:#B3B3B3;
}
.c-welfareImgUrl{
  width: 100%;
  height: 340rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}
.c-welfareImgUrl image{
  width: 100%;
  height: 100%;
  display: block;
}
.c-allTime{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color:#B3B3B3;
  font-size: 32rpx;
  border-bottom: 1rpx solid #F2F2F2;
}
.c-welfareStatus{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}
.c-welfareStatus view:nth-child(1){
  width: 140rpx;
  height: 36rpx;
  display: block;
  float: left;
  color:#FE9A1F;
  font-size: 32rpx;
  
  margin-top: 32rpx;
  line-height: 36rpx;
}
.c-welfareStatus view:nth-child(2){
  width: 2rpx;
  height: 36rpx;
  background:#FE9A1F ;
  float: left;
  margin-top: 32rpx;
}
.c-welfareStatus text:nth-child(3){
  width: 140rpx;
  height: 36rpx;
  display: block;
  float: left;
  color:#FE9A1F;
  font-size: 32rpx;
  margin-top: 32rpx;
  line-height: 36rpx;
  text-align: center;
}
.c-welfareStatus text:nth-child(4){
  width: 140rpx;
  height: 36rpx;
  display: block;
  float: right;
  color:#B3B3B3;
  font-size: 32rpx;
  margin-top: 32rpx;
  line-height: 36rpx;
  text-align: center;
}
.c-user{
  width: 200rpx;
  height: 100rpx;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  background: #fff;
  box-shadow:0px 0px 20rpx rgba(0,0,0,0.2);
  position:fixed;
  right:0;
  bottom:50rpx;
}
.c-user image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  display: block;
  position: absolute;
  top:0;
  bottom:0;
  left: 20rpx;
  margin: auto;
}
.c-news{
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: red;
  position: absolute;
  top: 15rpx;
  left: 80rpx
}